    "README.md"
  ],
  "dependencies": {
    "htmlclean": "^3.0.3",
    "lodash": "^4.9.0",
    "lodash-es": "^4.9.0",
    "rollup-pluginutils": "^1.3.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-es2015-rollup": "^1.1.1",
    "babel-register": "^6.7.2",
    "eslint": "^2.7.0",
    "mocha": "^2.4.5",
    "rollup": "^0.25.8",
    "rollup-plugin-babel": "^2.4.0"
  }
